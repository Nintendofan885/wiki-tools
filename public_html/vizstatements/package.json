{
  "private": true,
  "version": "0.1.0",
  "main": "index.js",
  "scripts": {
    "build": "npm run build:js && npm run build:css",
    "build:autoprefixer": "postcss style.css --use autoprefixer --no-map -r",
    "build:css": "npm run build:sass && npm run build:autoprefixer",
    "build:js": "webpack -d",
    "build:sass": "node-sass scss/style.scss style.css",
    "dist": "webpack -p",
    "postinstall": "npm run build",
    "watch": "chokidar 'scss/*.scss' -c 'npm run build:css' & webpack -dw"
  },
  "dependencies": {
    "regenerator-runtime": "^0.11.1",
    "valenski": "^1.3.0",
    "vue": "^2.5.16",
    "vue-simple-suggest": "^1.9.5"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.6",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "chokidar-cli": "^1.2.0",
    "css-loader": "^3.2.0",
    "http-server": "^0.11.1",
    "node-sass": "^4.9.0",
    "postcss": "^6.0.22",
    "postcss-cli": "^5.0.0",
    "vue-loader": "^15.0.9",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.8.1",
    "webpack-cli": "^3.1.1",
    "wikidata-ux" : "*"
  }
}